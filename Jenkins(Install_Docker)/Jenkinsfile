#!Groovy
pipeline {
    agent {
       node {
            label 'Ansible'
        }
    }
    stages {
        stage ('1. Show current path'){   
          steps{
              sh 'pwd'
        }
            }
        stage ('2.Install Docker') {
          steps{
            sh'''
            #!/bin/sh
            ansible-playbook /home/ansible_machine/workspace/Install_utilities/Ansible/install_Docker.yml
            '''
          }
        }


  }
}
